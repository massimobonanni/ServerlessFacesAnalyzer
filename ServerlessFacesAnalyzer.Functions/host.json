{
  "version": "2.0",
  "extensions": {
    "durableTask": {
      "hubName": "FaceAnalyzerHub"
    }
  },
  "logging": {
    "logLevel": {
      "default": "Information",
      "Host.Results": "Error",
      "Function": "Information",
      "Host.Aggregator": "Error"
    },
    "applicationInsights": {
      "samplingSettings": {
        "isEnabled": true,
        "excludedTypes": "Request"
      }
    }
  }
}